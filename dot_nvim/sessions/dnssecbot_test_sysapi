let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/src/systems-api
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +1 test/v3/370_dnssec_cryptokeys.t
badd +64 test/v3/380_zone_powerdns_metadatas.t
badd +62 test/v3/480_webconfig.t
badd +20 test/v3/390_dnssec_key_set.t
badd +1 test/v3/360_dns_service_records.t
argglobal
%argdel
$argadd test/v3/370_dnssec_cryptokeys.t
$argadd test/v3/380_zone_powerdns_metadatas.t
edit test/v3/360_dns_service_records.t
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 171 + 179) / 359)
exe 'vert 2resize ' . ((&columns * 187 + 179) / 359)
argglobal
if bufexists(fnamemodify("test/v3/360_dns_service_records.t", ":p")) | buffer test/v3/360_dns_service_records.t | else | edit test/v3/360_dns_service_records.t | endif
if &buftype ==# 'terminal'
  silent file test/v3/360_dns_service_records.t
endif
balt test/v3/480_webconfig.t
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 8 - ((0 * winheight(0) + 43) / 86)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 8
normal! 0
wincmd w
argglobal
if bufexists(fnamemodify("test/v3/390_dnssec_key_set.t", ":p")) | buffer test/v3/390_dnssec_key_set.t | else | edit test/v3/390_dnssec_key_set.t | endif
if &buftype ==# 'terminal'
  silent file test/v3/390_dnssec_key_set.t
endif
balt test/v3/360_dns_service_records.t
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 102 - ((85 * winheight(0) + 43) / 86)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 102
normal! 0
wincmd w
2wincmd w
exe 'vert 1resize ' . ((&columns * 171 + 179) / 359)
exe 'vert 2resize ' . ((&columns * 187 + 179) / 359)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=1
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
nohlsearch
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
